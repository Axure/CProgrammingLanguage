#include <stdio.h>
int even(int n) { return !(n&1); }
int main(void)
{
    int n, sum;
    int ri, repeat;

    scanf("%d", &repeat);
    for(ri = 1; ri <= repeat; ri++){	
	    sum = 0;
	while (1)
	{
        	scanf("%d",&n);
		if (n <= 0) break;
		sum += even(n)?0:n;
	}
        printf("The sum of the odd numbers is %d.\n", sum);
   }
}

